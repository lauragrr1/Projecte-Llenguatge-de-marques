{
  "$schema": "http://json-schema.org/draft-07/schema",
  "type": "object",
  "properties": {
    "date": {
      "type": "string",
      "pattern": "^\d{4}-\d{2}-\d{2} \d{2}:\d{2}:\d{2}$"
    },
    "start": {
      "type": "string",
      "format": "date-time"
    },
    "end": {
      "type": "string",
      "format": "date-time"
    },
    "name": {
      "type": "string"
    },
    "type": {
      "type": "string"
    },
    "rule": {
      "type": "string",
      "pattern": "^\d{2}-\d{2}$"
    }
  },
  "required": ["date", "start", "end", "name", "type", "rule"]
}