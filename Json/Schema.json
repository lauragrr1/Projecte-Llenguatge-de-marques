{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "type": "object",
    "items": {
      "type": "object",
      "properties": {
        "Date": { "type": "string", "format": "date" },
        "LocalName": { "type": "string" },
        "Name": { "type": "string" },
        "National": { "type": "boolean" },
        "SubdivisionCodes": {
          "type": "array",
          "items": { "type": "string" }
        }
      },
      "required": ["Date", "LocalName", "Name", "National", "SubdivisionCodes"]
    }
  }
  