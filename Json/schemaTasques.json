{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "Date": { "type": "string", "format": "date" },
            "Title": { "type": "string" },
            "Description": { "type": "string" },
            "Hour": { "type": "string" },
            "Status": { "type": "boolean" }
        },
        "required": ["Date", "Title", "Description", "Hour", "Status"]
    }
}